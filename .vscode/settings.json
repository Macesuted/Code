{
  "editor.formatOnType": true,
  "editor.suggest.snippetsPreventQuickSuggestions": true,
  "editor.acceptSuggestionOnEnter": "off",
  "editor.snippetSuggestions": "top",
  "code-runner.runInTerminal": true,
  "code-runner.executorMap": {
    "cpp": "cd $dir && run '$fileNameWithoutExt'"
  },
  "code-runner.saveFileBeforeRun": true,
  "code-runner.preserveFocus": false,
  "code-runner.clearPreviousOutput": false,
  "code-runner.ignoreSelection": true,
  "C_Cpp.clang_format_sortIncludes": true,
  "C_Cpp.clang_format_fallbackStyle": "{BasedOnStyle: Google, UseTab: Never, IndentWidth: 4, TabWidth: 4, UseTab: Never, ColumnLimit: 0}",
  "files.autoSave": "onFocusChange",
  "editor.cursorSmoothCaretAnimation": true,
  "editor.smoothScrolling": true,
  "workbench.list.smoothScrolling": true,
  "editor.fontFamily": "Fira Code",
  "editor.fontLigatures": true,
  "editor.fontWeight": "600",
  "files.associations": {
    "chrono": "cpp",
    "random": "cpp",
    "limits": "cpp",
    "valarray": "cpp",
    "ostream": "cpp",
    "cmath": "cpp",
    "iosfwd": "cpp",
    "tuple": "cpp",
    "deque": "cpp",
    "string": "cpp",
    "vector": "cpp",
    "iostream": "cpp",
    "*.tcc": "cpp",
    "forward_list": "cpp",
    "list": "cpp",
    "unordered_map": "cpp",
    "unordered_set": "cpp",
    "any": "cpp",
    "system_error": "cpp",
    "istream": "cpp",
    "complex": "cpp",
    "iomanip": "cpp"
  },
  "doxdocgen.generic.authorEmail": "i@macesuted.moe",
  "doxdocgen.generic.authorName": "Macesuted",
  "doxdocgen.file.versionTag": "",
  "doxdocgen.generic.briefTemplate": "",
  "editor.bracketPairColorization.enabled": true,
  "terminal.integrated.profiles.windows": {
    "PowerShell": null,
    "JavaScript Debug Terminal": null,
    "pwsh": {
      "path": "pwsh.exe",
      "icon": "terminal-powershell"
    }
  },
  "terminal.integrated.defaultProfile.windows": "pwsh"
}